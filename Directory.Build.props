<Project>
  <PropertyGroup>
   <Authors>Lutando Ngqakaza</Authors>
    <Company>Lutando Ngqakaza</Company>
    <Copyright>Copyright (c) Lutando Ngqakaza 2018 - 2019</Copyright>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Lutando/EventFly</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <NeutralLanguage>en-GB</NeutralLanguage>
    <PackageReleaseNotes>Externally Updated</PackageReleaseNotes>
    <PackageIconUrl>https://EventFly.net/logos/logo-512.png</PackageIconUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>
    
  <PropertyGroup>
      <NoWarn>NU5104</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <NetCoreAppVersion>netcoreapp2.2</NetCoreAppVersion>
    <NetStandardVersion>netstandard2.0</NetStandardVersion>
    <AkkaVersion>1.3.14</AkkaVersion>
    <AkkaPersistenceVersion>1.3.14</AkkaPersistenceVersion>
    <AkkaPersistenceQueryVersion>1.3.14</AkkaPersistenceQueryVersion>
    <AkkaRemoteVersion>1.3.14</AkkaRemoteVersion>
    <AkkaRemoteTestKitVersion>1.3.14</AkkaRemoteTestKitVersion>
    <AkkaClusterVersion>1.3.14</AkkaClusterVersion>
    <AkkaClusterShardingVersion>1.3.13-beta</AkkaClusterShardingVersion>
    <AkkaClusterToolsVersion>1.3.14</AkkaClusterToolsVersion>
    <AkkaTestKitVersion>1.3.14</AkkaTestKitVersion>
    <AkkaClusterTestKitVersion>1.3.14</AkkaClusterTestKitVersion>
    <AkkaTestKitXunitVersion>1.3.14</AkkaTestKitXunitVersion>
    <XunitVersion>2.4.1</XunitVersion>
    <XunitRunnerVSVersion>2.4.1</XunitRunnerVSVersion>
    <XunitRunnerUtilityVersion>2.4.1</XunitRunnerUtilityVersion>
    <CoverletMSBuildVersion>2.6.3</CoverletMSBuildVersion>
    <FluentAssertionsVersion>5.9.0</FluentAssertionsVersion>
    <TestSdkVersion>16.3.0</TestSdkVersion>
    <SourceLinkGithubVersion>1.0.0-beta2-19367-01</SourceLinkGithubVersion>
  </PropertyGroup>

  <PropertyGroup>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>
</Project>